{
  "presets": [
    "es2015"
  ],
  "plugins": [
    "@babel/plugin-proposal-export-namespace-from",
    "transform-class-properties",
    "transform-react-jsx",
    "transform-object-rest-spread",
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ]
      }
    ],
    [
      "import-rename",
      {
        "^(.*)\\.jsx$": "$1"
      }
    ]
  ]
}